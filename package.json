{
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "name": "Election",
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "prebuild": "npm run prebuild --workspaces --if-present",
    "pretest": "npm run prebuild --workspaces --if-present",
    "start": "npm start --workspaces --if-present",
    "test": "npm test --workspaces --if-present",
    "lint": "eslint src/Election_frontend --ext .js,.jsx",
    "lint:fix": "eslint src/Election_frontend --ext .js,.jsx --fix",
    "vitest": "vitest",
    "vitest:ui": "vitest --ui",
    "ci": "vitest run",
    "preci": "dfx stop; dfx start --background --clean; dfx deploy; dfx generate",
    "format": "prettier --write \"src/**/*.{js,jsx,tsx,json,css,md}\"",
    "mo-test":"mops test"
  },
  "type": "module",
  "workspaces": [
    "src/Election_frontend"
  ],
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@vitest/ui": "^3.1.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.26.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.0.0",
    "isomorphic-fetch": "^3.0.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.1.2"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.17",
    "@fullcalendar/interaction": "^6.1.17",
    "@fullcalendar/list": "^6.1.17",
    "react-hook-form": "^7.56.1",
    "react-router": "^7.5.2",
    "react-router-dom": "^7.5.2"
  }
}
